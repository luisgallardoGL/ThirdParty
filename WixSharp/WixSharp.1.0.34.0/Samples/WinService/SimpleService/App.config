<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- 1M = 1048576B -->
    <add key="App.LogFileMaxSize" value="1048576" />
    <add key="App.LogFileNumOfFilesToKeep" value="3" />
    <add key="App.UseParallel" value="true" />
    <add key="GZip.InputFileFilter" value="*.gz" />
    <add key="GZip.ForcedProcessingDelay" value="1000" />
    <add key="GZip.LogFileNumOfDaysToKeep" value="5" />
    <add key="GZip.InDir" value="Testing\TestResults\GZip\Input" />
    <add key="GZip.OutDir" value="Testing\TestResults\GZip\Out" />
    <add key="GZip.ErrorDir" value="Testing\TestResults\GZip\ErrorLog" />
    <add key="GZip.LogDir" value="Testing\TestResults\GZip\Log" />
    <!--<add key="GZip.TroubleshootingDir" value="Testing\GZip\Troubleshooting" />-->
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" />
  </startup>
</configuration>
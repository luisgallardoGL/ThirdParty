                                    Visibility="Collapsed"
                                    Width="9"
                                    Height="9"
                                    Stretch="None"
                                    StrokeThickness="1.5"
                                    Stroke="{StaticResource CheckBoxTickStroke_Normal}"
                                    Data="M2,4 L 7,4"/>
                            <telerikChromes:ValidationTooltip x:Name="ValidationTooltip"

                                        <ContentPresenter Content="{TemplateBinding AdditionalContent}" Grid.Column="4"/>
                                    </Grid>
                                </Grid>
                            </telerikPrimitives:LayoutTransformControl>
                            <Border x:Name="ContentBorder"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Background="{TemplateBinding Background}"
                                    Padding="{TemplateBinding Padding}">
                                <Grid>
                                    <ContentPresenter x:Name="ContentElement" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource InvertBoolToVisConverter}}" ContentTemplate="{TemplateBinding SelectedContentTemplate}"/>
                                    <Grid x:Name="ContentElementsPanel" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource BoolToVisConverter}}"/>
                                </Grid>
